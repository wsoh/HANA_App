{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"websockets": {
		"enabled": true
	},
	"routes": [ {
 		"source": "/node(.*)",
 		"destination": "core-backend",
 		"csrfProtection": true,
 		"authenticationType": "xsuaa"
 	}, {
		"source": "(.*)(.xsjs)",
		"destination": "core-xsjs-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "core-xsjs-backend",
		"authenticationType": "xsuaa",
		"csrfProtection": false
	}, {
		"source": "/(.*)",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"replace": {
			"pathSuffixes": ["index.html", "odataTest.html"],
			"services": {
				"sapui5_sb": {
					"tag": "ui5"
				}
			}		
		}
	}]
}